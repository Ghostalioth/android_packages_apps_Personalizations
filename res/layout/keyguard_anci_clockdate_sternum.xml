<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:gravity="end"
    android:layout_gravity="end"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="50.0dip" />
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextClock
            android:textSize="24.0dip"
            android:textColor="@android:color/system_accent1_100"
            android:id="@+id/lsclock9_d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:format12Hour="MMMM dd "
            android:format24Hour="MMMM dd " />
        <LinearLayout
            android:layout_gravity="center"
            android:background="@drawable/anci_dot_bg"
            android:layout_width="5.0dip"
            android:layout_height="25.0dip" />
        <TextClock
            android:textSize="24.0dip"
            android:textColor="@android:color/system_accent1_100"
            android:id="@+id/lsclock9_e"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:format12Hour=" EEEE"
            android:format24Hour=" EEEE" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextClock
            android:textSize="100.0dip"
            android:textColor="@android:color/system_accent1_100"
            android:id="@+id/lsclock9_a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:format12Hour="hh"
            android:format24Hour="HH" />
        <TextClock
            android:textSize="100.0dip"
            android:textColor="@android:color/system_accent1_100"
            android:id="@+id/lsclock9_b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:format12Hour=":"
            android:format24Hour=":" />
        <TextClock
            android:textSize="100.0dip"
            android:textColor="@android:color/system_accent1_100"
            android:id="@+id/lsclock9_c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:format12Hour="mm"
            android:format24Hour="mm" />
    </LinearLayout>
</LinearLayout>